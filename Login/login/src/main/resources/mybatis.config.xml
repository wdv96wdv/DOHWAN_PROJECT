<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <!-- underscore_to_camel 설정 -->
    <settings>
        <setting name="mapUnderscoreToCamelCase" value="true"/>
    </settings>

    <!-- TypeHandler 등록 -->
    <typeHandlers>
        <typeHandler javaType="com.dohwan.board.domain.Files$FileType" handler="org.apache.ibatis.type.EnumTypeHandler"/>
    </typeHandlers>
</configuration>
